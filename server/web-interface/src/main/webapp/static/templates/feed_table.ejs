<% 
    for (var key=0; key<feeds.length;key++) {
        var feed = feeds[key];
        var date = new Date(feed.date);
        var date_str = date.getDate() + "/" + date.getMonth() + "/" + date.getFullYear();;
%>
<div class="left" onmouseover="$(this).find('.hint').removeClass('hidden')" onmouseout="$(this).find('.hint').addClass('hidden');">
    <div class="hidden hint">
        <table cellpadding="0" cellspacing="0" style="width: 292px;" summary="">
            <tr class="black_10">
                <td>Name:</td>
                <td><%=feed.name%></td>
            </tr>
            <tr class="black_20">
                <td>Taken:</td>
                <td><%=date_str%></td>
            </tr>
            <tr class="black_10">
                <td>Point name:</td>
                <td><%=feed.point%></td>
            </tr>
        </table>
    </div>
    <div class="feed_container <%= key%2==0? "feed_left_block" : "" %>">
        <div class="photo"><img src="<%= feed.photo %>" width="292" height="254" alt=""/></div>
        <div class="description"><%= feed.message %></div>
    </div>
</div>
<%
    }
%>

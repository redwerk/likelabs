<%
    for (var key=0; key<photos.length;key++) {
        var photo = photos[key];
%>
<div class="item-wrapper photo" id="photo_item_<%=photo.id%>" style="height: 330px;">
    <div class="item-data">
        <div class="photo" style="text-align: center" id="photo_<%= key %>"><img src="/public/photo/<%=photo.id%>" alt=""/></div>
        <div class="description btn-holder" style="text-align: center;">
            <button style="width: 120px; height: 32px;" type="button" class="btn <%= photo.status == 'active'? 'disabled' : 'btn-success'%> publish" onclick="changeStatusPhoto(<%=photo.id %>,'active')">Activate</button>
            <button style="width: 120px; height: 32px;" type="button" class="btn <%= photo.status == 'deleted'? 'disabled' : 'btn-success'%> publish" onclick="changeStatusPhoto(<%=photo.id %>,'deleted')">Delete</button>
        </div>
    </div>
</div>
<%
    }
%>
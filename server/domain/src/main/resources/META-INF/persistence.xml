<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="likelabs" transaction-type="RESOURCE_LOCAL">
        <provider>org.hibernate.ejb.HibernatePersistence</provider>
        <class>com.redwerk.likelabs.domain.model.user.User</class>
        <class>com.redwerk.likelabs.domain.model.user.UserSocialAccount</class>
        <class>com.redwerk.likelabs.domain.model.company.Company</class>
        <class>com.redwerk.likelabs.domain.model.company.CompanySocialPage</class>
        <class>com.redwerk.likelabs.domain.model.company.NotificationIntervals</class>
        <class>com.redwerk.likelabs.domain.model.point.Point</class>
        <class>com.redwerk.likelabs.domain.model.point.Address</class>
        <class>com.redwerk.likelabs.domain.model.tablet.Tablet</class>
        <class>com.redwerk.likelabs.domain.model.settings.SystemParameter</class>
        <class>com.redwerk.likelabs.domain.model.event.Event</class>
        <class>com.redwerk.likelabs.domain.model.review.Review</class>
        <class>com.redwerk.likelabs.domain.model.review.Recipient</class>
        <class>com.redwerk.likelabs.domain.model.photo.Photo</class>
    </persistence-unit>
</persistence>